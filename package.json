{
  "name": "touchfish-book",
  "displayName": "touchfish-book",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "touchfish-book.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "touchfish-book.zzh",
        "title": "打开看书标签"
      },
      {
        "command": "touchfish-book.refreshQidian",
        "title": "刷新起点三江",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "touchfish-book.refreshQidian",
          "when": "view == qidian",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "touchfish-book",
          "title": "摸鱼看书",
          "icon": "resources/book.svg"
        }
      ]
    },
    "views": {
      "touchfish-book": [
        {
          "id": "qidian",
          "name": "起点"
        },
        {
          "id": "jinjiang",
          "name": "晋江"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.1.2"
  }
}
